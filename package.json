{
  "name": "electron_demo",
  "version": "1.0.0",
  "description": "electron_demo",
  "main": "main.ts",
  "scripts": {
    "start": "cross-env NODE_ENV=development node ./build/scripts/start.js",
    "start:render": "cross-env NODE_ENV=development webpack --config ./build/configs/webpack.dev.js",
    "start:preload": "cross-env NODE_ENV=development webpack --config ./build/configs/webpack.preload.js",
    "build": "cross-env NODE_ENV=development node ./build/scripts/build.js --dir --config=./electron-builder.json5",
    "build:pack": "cross-env NODE_ENV=production node ./build/scripts/build.js --dir --config=./electron-builder.json5",
    "build:dist": "cross-env NODE_ENV=production node ./build/scripts/build.js --config=./electron-builder.json5",
    "pack": "electron-builder --dir ",
    "dist": "electron-builder --config=./electron-builder.json5",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx --fix",
    "rebuild": "electron-rebuild -f -a x64 -v 15.3.1 -w sqlite3"
  },
  "author": "lsq",
  "license": "ISC",
  "devDependencies": {
    "@babel/cli": "^7.16.0",
    "@babel/core": "^7.16.0",
    "@babel/plugin-proposal-class-properties": "^7.16.0",
    "@babel/preset-env": "^7.16.0",
    "@babel/preset-react": "^7.16.0",
    "@babel/preset-typescript": "^7.16.0",
    "@types/lodash": "^4.14.176",
    "@types/md5": "^2.3.1",
    "@types/react": "^17.0.34",
    "@types/react-dom": "^17.0.11",
    "@types/react-redux": "^7.1.20",
    "@types/react-router-config": "^5.0.3",
    "@types/react-router-dom": "^5.3.2",
    "@types/redux": "^3.6.31",
    "@types/sqlite3": "^3.1.7",
    "@types/uuid": "^8.3.1",
    "@typescript-eslint/eslint-plugin": "^5.3.1",
    "@typescript-eslint/parser": "^5.3.1",
    "babel-loader": "^8.2.3",
    "copy-webpack-plugin": "^9.0.1",
    "core-js": "^3.19.1",
    "cross-env": "^7.0.3",
    "css-loader": "^6.5.1",
    "electron": "^15.3.1",
    "electron-builder": "^22.13.1",
    "electron-rebuild": "^3.2.5",
    "eslint": "^8.2.0",
    "eslint-config-airbnb": "^19.0.0",
    "eslint-config-airbnb-typescript": "^15.0.0",
