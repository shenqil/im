{
  "name": "e_chat",
  "version": "1.0.0",
  "description": "electron chat",
  "main": "main.ts",
  "scripts": {
    "dev": "npm run start",
    "start": "cross-env NODE_ENV=development node ./build/scripts/start.js",
    "start:render": "cross-env NODE_ENV=development webpack --config ./build/configs/webpack.dev.js",
    "start:preload": "cross-env NODE_ENV=development webpack --config ./build/configs/webpack.preload.js",
    "build": "cross-env NODE_ENV=production node ./build/scripts/build.js --dir --config=./electron-builder.json5",
    "build:development": "cross-env NODE_ENV=development node ./build/scripts/build.js --dir --config=./electron-builder.json5",
    "build:pack": "cross-env NODE_ENV=production node ./build/scripts/build.js --dir --config=./electron-builder.json5",
    "build:dist": "cross-env NODE_ENV=production node ./build/scripts/build.js --config=./electron-builder.json5",
    "pack": "electron-builder --dir ",
    "dist": "electron-builder --config=./electron-builder.json5",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx --fix",
    "rebuild": "electron-rebuild -f -a x64 -v 15.3.1 -w sqlite3",
    "electron:generate-icons": "electron-icon-builder --input=./src/static/img/logo.png --output=./build --flatten"
  },
  "author": "lsq",
  "license": "ISC",
  "devDependencies": {
    "@babel/cli": "^7.16.0",
    "@babel/core": "^7.16.0",
    "@babel/plugin-proposal-class-properties": "^7.16.0",
    "@babel/preset-env": "^7.16.4",
    "@babel/preset-react": "^7.16.0",
    "@babel/preset-typescript": "^7.16.0",
    "@types/lodash": "^4.14.177",
    "@types/md5": "^2.3.1",
    "@types/react": "^17.0.37",
    "@types/react-dom": "^17.0.11",
    "@types/react-redux": "^7.1.20",
    "@types/redux": "^3.6.31",
    "@types/redux-logger": "^3.0.9",
    "@types/sqlite3": "^3.1.7",
    "@types/throttle-debounce": "^2.1.0",
    "@types/uuid": "^8.3.3",
    "@typescript-eslint/eslint-plugin": "^5.5.0",
    "@typescript-eslint/parser": "^5.5.0",
    "babel-loader": "^8.2.3",
    "core-js": "^3.19.2",
    "cross-env": "^7.0.3",
    "css-loader": "^6.5.1",
    "electron": "^15.3.1",
    "electron-builder": "^22.14.5",
    "electron-devtools-installer": "^3.2.0",
    "electron-icon-builder": "^2.0.1",
    "electron-rebuild": "^3.2.5",
    "eslint": "^8.3.0",
    "eslint-config-airbnb": "^19.0.1",
    "eslint-config-airbnb-typescript": "^16.0.0",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-react": "^7.27.1",
    "eslint-plugin-react-hooks": "^4.3.0",
    "eslint-webpack-plugin": "^3.1.1",
    "fs-extra": "^10.0.0",
    "html-loader": "^3.0.1",
    "html-webpack-plugin": "^5.5.0",
    "mini-css-extract-plugin": "^2.4.5",
    "node-loader": "^2.0.0",
    "sass": "^1.44.0",
    "sass-loader": "^12.3.0",
    "style-loader": "^3.3.1",
    "ts-loader": "^9.2.6",
    "typescript": "^4.5.2",
    "webpack": "^5.64.4",
    "webpack-cli": "^4.9.1",
    "webpack-dev-server": "^4.6.0",
    "webpack-merge": "^5.8.0"
  },
  "dependencies": {
    "@ant-design/icons": "^4.7.0",
    "@reduxjs/toolkit": "^1.6.2",
    "antd": "^4.17.2",
    "lodash": "^4.17.21",
    "md5": "^2.3.0",
    "moment": "^2.29.1",
    "mqtt": "^4.2.8",
    "pinyin-match": "^1.2.2",
    "pinyin-pro": "^3.3.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-redux": "^7.2.6",
    "react-router-dom": "^6.0.2",
    "redux": "^4.1.2",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.4.1",
    "sqlite3": "^5.0.2",
    "throttle-debounce": "^3.0.1",
    "tus-js-client": "^2.3.0",
    "uuid": "^8.3.2"
  }
}
