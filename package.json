{
  "name": "electron_demo",
  "version": "1.0.0",
  "description": "electron_demo",
  "main": "main.ts",
  "scripts": {
    "start": "cross-env NODE_ENV=development node ./build/scripts/start.js",
    "start:render": "cross-env NODE_ENV=development webpack --config ./build/configs/webpack.dev.js",
    "start:preload": "cross-env NODE_ENV=development webpack --config ./build/configs/webpack.preload.js",
    "build": "cross-env NODE_ENV=development node ./build/scripts/build.js --dir --config=./electron-builder.json5",
    "build:pack": "cross-env NODE_ENV=production node ./build/scripts/build.js --dir --config=./electron-builder.json5",
    "build:dist": "cross-env NODE_ENV=production node ./build/scripts/build.js --config=./electron-builder.json5",
    "pack": "electron-builder --dir ",
    "dist": "electron-builder --config=./electron-builder.json5",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx"
  },
  "author": "lsq",
  "license": "ISC",
  "devDependencies": {
    "@babel/cli": "^7.14.5",
    "@babel/core": "^7.14.5",
    "@babel/plugin-proposal-class-properties": "^7.14.5",
    "@babel/preset-env": "^7.14.5",
    "@babel/preset-react": "^7.14.5",
    "@babel/preset-typescript": "^7.14.5",
    "@types/lodash": "^4.14.170",
    "@types/react": "^17.0.11",
    "@types/react-dom": "^17.0.7",
    "@typescript-eslint/eslint-plugin": "^4.4.1",
    "@typescript-eslint/parser": "^4.4.1",
    "babel-loader": "^8.2.2",
    "copy-webpack-plugin": "^9.0.0",
    "core-js": "^3.14.0",
    "cross-env": "^7.0.3",
    "css-loader": "^6.3.0",
    "electron": "^14.0.1",
    "electron-builder": "^22.11.7",
    "eslint": "^7.28.0",
    "eslint-config-airbnb-typescript": "^14.0.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-react": "^7.20.3",
    "eslint-plugin-react-hooks": "^4.0.8",
    "eslint-webpack-plugin": "^3.0.1",
    "html-loader": "^2.1.2",
    "html-webpack-plugin": "^5.3.1",
    "mini-css-extract-plugin": "^2.3.0",
    "node-loader": "^2.0.0",
    "sass": "^1.34.1",
    "sass-loader": "^12.0.0",
    "style-loader": "^3.2.1",
    "ts-loader": "^9.2.2",
    "typescript": "^4.0.7",
    "webpack": "^5.38.1",
    "webpack-cli": "^4.7.0",
    "webpack-dev-server": "^4.2.1",
    "webpack-merge": "^5.7.3"
  },
  "dependencies": {
    "lodash": "^4.17.21",
    "moment": "^2.29.1",
    "mqtt": "^4.2.8",
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  }
}
