{
  "name": "electron_demo",
  "version": "1.0.0",
  "description": "electron_demo",
  "main": "main.ts",
  "scripts": {
    "start": "cross-env NODE_ENV=development node ./build/scripts/start.js",
    "start:render": "cross-env NODE_ENV=development webpack --config ./build/configs/webpack.dev.js",
    "start:preload": "cross-env NODE_ENV=development webpack --config ./build/configs/webpack.preload.js",
    "build": "cross-env NODE_ENV=development node ./build/scripts/build.js --dir --config=./electron-builder.json5",
    "build:pack": "cross-env NODE_ENV=production node ./build/scripts/build.js --dir --config=./electron-builder.json5",
    "build:dist": "cross-env NODE_ENV=production node ./build/scripts/build.js --config=./electron-builder.json5",
    "pack": "electron-builder --dir ",
    "dist": "electron-builder --config=./electron-builder.json5",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx --fix"
  },
  "author": "lsq",
  "license": "ISC",
  "devDependencies": {
    "@babel/cli": "^7.15.7",
    "@babel/core": "^7.15.8",
    "@babel/plugin-proposal-class-properties": "^7.14.5",
    "@babel/preset-env": "^7.15.8",
    "@babel/preset-react": "^7.14.5",
    "@babel/preset-typescript": "^7.15.0",
    "@types/lodash": "^4.14.175",
    "@types/react": "^17.0.27",
    "@types/react-dom": "^17.0.9",
    "@typescript-eslint/eslint-plugin": "^4.33.0",
    "@typescript-eslint/parser": "^4.33.0",
    "babel-loader": "^8.2.2",
    "copy-webpack-plugin": "^9.0.1",
    "core-js": "^3.18.2",
    "cross-env": "^7.0.3",
    "css-loader": "^6.3.0",
    "electron": "^15.1.1",
    "electron-builder": "^22.11.7",
    "eslint": "^7.32.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-airbnb-typescript": "^14.0.0",
    "eslint-plugin-import": "^2.24.2",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-react": "^7.26.1",
    "eslint-plugin-react-hooks": "^4.2.0",
    "eslint-webpack-plugin": "^3.0.1",
    "html-loader": "^2.1.2",
    "html-webpack-plugin": "^5.3.2",
    "mini-css-extract-plugin": "^2.4.2",
    "node-loader": "^2.0.0",
    "sass": "^1.42.1",
    "sass-loader": "^12.1.0",
    "style-loader": "^3.3.0",
    "ts-loader": "^9.2.6",
    "typescript": "^4.4.3",
    "webpack": "^5.58.0",
    "webpack-cli": "^4.9.0",
    "webpack-dev-server": "^4.3.1",
    "webpack-merge": "^5.8.0"
  },
  "dependencies": {
    "lodash": "^4.17.21",
    "moment": "^2.29.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  }
}
